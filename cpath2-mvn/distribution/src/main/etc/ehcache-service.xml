<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
	updateCheck="false"> 
	<!--
		| Please see
		http://ehcache.sourceforge.net/documentation/configuration.html for |
		detailed information on how to configurigure caches in this file +
	--> 
	<!-- Location of persistent caches on disk --> 
	<diskStore path="java.io.tmpdir/cpath2_ws_cache" /> 
 
	<defaultCache eternal="false" maxElementsInMemory="10000"
        overflowToDisk="true" diskPersistent="false" timeToIdleSeconds="0"
        timeToLiveSeconds="600" memoryStoreEvictionPolicy="LRU"/> 
 
 
	<!--
		EXAMPLE ENTRIES
		
		<cache name="weatherCache" eternal="false"
		maxElementsInMemory="100" overflowToDisk="false"
		diskPersistent="false" timeToIdleSeconds="0" timeToLiveSeconds="300"
		memoryStoreEvictionPolicy="LRU" /> <cache name="locationSearchCache"
		eternal="false" maxElementsInMemory="100" overflowToDisk="false"
		diskPersistent="false" timeToIdleSeconds="0" timeToLiveSeconds="300"
		memoryStoreEvictionPolicy="LRU" />
	--> 
	
		<cache name="elementByIdCache" eternal="false" maxElementsInMemory="10000"
        overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
        timeToLiveSeconds="300" memoryStoreEvictionPolicy="LRU"/> 

        
        <cache name="findElementsCache" eternal="false" maxElementsInMemory="10000"
        overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
        timeToLiveSeconds="300" memoryStoreEvictionPolicy="LRU"/>
        
		<cache name="findEntitiesCache" eternal="false" maxElementsInMemory="10000"
        overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
        timeToLiveSeconds="300" memoryStoreEvictionPolicy="LRU"/>
        
        
		<cache name="getCommonStreamCache" eternal="false" maxElementsInMemory="10000"
        overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
        timeToLiveSeconds="300" memoryStoreEvictionPolicy="LRU"/>
        
 		<cache name="getPathsBetweenCache" eternal="false" maxElementsInMemory="10000"
        overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
        timeToLiveSeconds="300" memoryStoreEvictionPolicy="LRU"/> 
        
		<cache name="getNeighborhoodCache" eternal="false" maxElementsInMemory="10000"
        overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
        timeToLiveSeconds="300" memoryStoreEvictionPolicy="LRU"/>     
	
</ehcache>