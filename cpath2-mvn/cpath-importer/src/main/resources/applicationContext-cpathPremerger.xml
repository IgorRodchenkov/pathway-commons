<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:annotation-config/>

	<bean id="premergeDispatcher" class="cpath.importer.internal.PremergeDispatcher">
		<constructor-arg ref="metadataDAO"/>
	</bean>

	<bean id="premerge" class="cpath.importer.internal.PremergeImpl" scope="prototype">
	  <constructor-arg ref="pathwayDataDAO"/>
	  <constructor-arg ref="validator"/>
	</bean>

</beans>
